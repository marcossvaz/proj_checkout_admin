generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model Attribute {
  id         String   @id @default(uuid())
  name       String
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  value_attribute AttributeValue[]

}

model AttributeValue {
  id String @id @default(uuid())
  id_attribute String
  value Float
  created_at DateTime @default(now())
  updated_at DateTime @updatedAt

  attribute Attribute @relation(fields: [id_attribute], references: [id])

  @@map("attributes_values")
}
